task calculator(a, b, op) {
    when op equal "+" {
        give a + b
    }
    when op equal "-" {
        give a - b
    }
    when op equal "*" {
        give a * b
    }
    when op equal "/" {
        give a / b
    }
    give 0
}

out calculator(10, 5, "+")   // 15
out calculator(10, 5, "*")   // 50
